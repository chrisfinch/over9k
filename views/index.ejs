		</section>

		<!-- 3D PROJECTS CAROUSEL -->
		<section id="projects">
			<div id="prj_center">
				<div id="prj_carousel_cont">
					<ul id="project_carousel">
						<% for (var i=0;i<projects.length;i++) {
							p = projects[i]; %>
							<li class="project">
								<div class="img"><img src="<%=p.image %>" /></div>
								<div class="text">
									<h4><%=p.title %></h4>
									<span class="client">For: <%=p.client %></span>
									<span class="sep">|</span>
									<span class="url"><a href="<%=p.url %>"><%=p.url %></a></span>
									<div class="description"><%-p.niceDesc() %></div>
								</div>
							</li>
						<% } %>
					</ul>
				</div>
				<div id="navigation_prj">
					<button id="previous" data-increment="-1" class="iconic arrow-up"></button>
					<button id="next" data-increment="1" class="iconic arrow-bottom"></button>
				</div>
			</div>
		</section>

		<!-- CONTACT FORM -->
		<section id="contact">
			<div id="alert-success" class="alert alert-success hide">
				<span><strong>Thankyou.</strong> Your message has been sent and will be responded to as soon as possible.</span>
			</div>

			<div id="alert-error" class="alert alert-error hide">
				<span><strong>There was an error sending your message.</strong> Please try again.</span>
			</div>

			<div class="desc side">
				<h2>Contact</h2>
				<p>Please complete this form to contact >9k or Chris.</p>
			</div>

			<div class="form main">
				<form method="post" action="contact" class="form-horizontal">

					<div class="control-group">
						<label class="control-label" for="name">Name</label>
						<div class="controls">
							<input id="name" class="text" name="name" type="text" placeholder="Name" />
						</div>
					</div>

					<div class="control-group">
						<label class="control-label" for="email">Email</label>
						<div class="controls">
							<input id="email" class="text" name="email" type="email" placeholder="Email" />
						</div>
					</div>

					<div class="control-group">
						<label class="control-label" for="message">Message</label>
						<div class="controls">
							<textarea id="message" class="text" name="message" placeholder="Message"></textarea>
						</div>
					</div>

					<div class="control-group">
						<div class="controls">
							<button id="submit" type="submit" class="btn">Send</button><img id="loader" src="/img/ajax-loader.gif" />
						</div>
					</div>

				</form>
			</div>
		</section>

  	</div>

